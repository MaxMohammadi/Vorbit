{
  "name": "three-geojson-geometry",
  "version": "1.1.0",
  "description": "ThreeJS geometry for stroking GeoJSON objects on a sphere",
  "unpkg": "dist/three-geojson-geometry.min.js",
  "main": "dist/three-geojson-geometry.common.js",
  "module": "dist/three-geojson-geometry.module.js",
  "types": "dist/three-geojson-geometry.d.ts",
  "homepage": "https://github.com/vasturiano/three-geojson-geometry",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/three-geojson-geometry.git"
  },
  "bugs": {
    "url": "https://github.com/vasturiano/three-geojson-geometry/issues"
  },
  "license": "MIT",
  "keywords": [
    "3d",
    "three",
    "polygons",
    "sphere",
    "stroke",
    "webgl",
    "geojson"
